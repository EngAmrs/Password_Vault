<h1>Register</h1>

<%= form_with model: @user, url: users_path, method: :post do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.email_field :username %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Register" %>
  </div>
<% end %>
